{
  "name": "devcrate",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "prebuild": "npx ts-node  -r tsconfig-paths/register --project ../tools/tsconfig.json ../tools/prebuild-app.ts",
    "prestart": "npm run prebuild",
    "start": "ng serve",
    "build": "ng build",
    "predeploy": "npm run prebuild",
    "deploy": "npx ng deploy",
    "prebuild:prod": "npm run prebuild",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "build:ngx-dc-styles": "cd ./projects/devcrate/ngx-dc-styles && npm run build",
    "build:libs": "run-s build:ngx-dc-utils build:ngx-dc-data-sources build:remaining",
    "build:remaining": "run-p --max-parallel 5 --print-label build:ngx-dc-demo-card build:ngx-dc-file-viewer build:ngx-dc-mat-select-data-source build:ngx-dc-navbar build:ngx-dc-side-pane-list build:ngx-dc-dropdown build:ngx-dc-styles",
    "build:ngx-dc-utils": "ng build @devcrate/ngx-dc-utils --configuration=production",
    "build:ngx-dc-data-sources": "ng build @devcrate/ngx-dc-data-sources --configuration=production",
    "build:ngx-dc-demo-card": "ng build @devcrate/ngx-dc-demo-card --configuration=production",
    "build:ngx-dc-file-viewer": "ng build @devcrate/ngx-dc-file-viewer --configuration=production",
    "build:ngx-dc-mat-select-data-source": "ng build @devcrate/ngx-dc-mat-select-data-source --configuration=production",
    "build:ngx-dc-navbar": "ng build @devcrate/ngx-dc-navbar --configuration=production",
    "build:ngx-dc-side-pane-list": "ng build @devcrate/ngx-dc-side-pane-list --configuration=production",
    "build:ngx-dc-dropdown": "ng build @devcrate/ngx-dc-dropdown --configuration=production",
    "watch:utils": "npm run build:utils -- --watch",
    "watch:ngx-dc-data-sources": "npm run build:ngx-dc-data-sources -- --watch",
    "watch:ngx-dc-dropdown": "npm run build:ngx-dc-dropdown -- --watch",
    "watch:ngx-dc-side-pane-list": "npm run build:ngx-dc-side-pane-list -- --watch",
    "watch:ngx-dc-file-viewer": "npm run build:ngx-dc-file-viewer -- --watch",
    "watch:ngx-dc-navbar": "npm run build:ngx-dc-navbar -- --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.6",
    "@angular/cdk": "^20.2.9",
    "@angular/common": "^20.3.6",
    "@angular/compiler": "^20.3.6",
    "@angular/core": "^20.3.6",
    "@angular/forms": "^20.3.6",
    "@angular/material": "^20.2.9",
    "@angular/platform-browser": "^20.3.6",
    "@angular/platform-browser-dynamic": "^20.3.6",
    "@angular/router": "^20.3.6",
    "highlight.js": "^11.10.0",
    "marked": "^15.0.0",
    "ng2-pdf-viewer": "^10.4.0",
    "ngx-highlightjs": "^12.0.0",
    "ngx-markdown": "^20.0.0",
    "prismjs": "^1.28.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "20.4.0",
    "@angular-eslint/eslint-plugin": "20.4.0",
    "@angular-eslint/eslint-plugin-template": "20.4.0",
    "@angular-eslint/schematics": "20.4.0",
    "@angular-eslint/template-parser": "20.4.0",
    "@angular/build": "^20.3.6",
    "@angular/cli": "^20.3.6",
    "@angular/compiler-cli": "^20.3.6",
    "@playwright/test": "^1.48.0",
    "@types/argparse": "^2.0.16",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "angular-cli-ghpages": "^2.0.3",
    "argparse": "^2.0.1",
    "cpx": "^1.5.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb-typescript": "~18.0.0",
    "eslint-plugin-deprecation": "1.5.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsdoc": "^48.0.2",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-unused-imports": "^3.2.0",
    "jasmine-core": "~5.2.0",
    "jest": "^29.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.3.0",
    "npm-run-all": "^4.1.5",
    "schematics-scss-migrate": "^2.3.17",
    "ts-jest": "^29.2.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.9.3"
  }
}